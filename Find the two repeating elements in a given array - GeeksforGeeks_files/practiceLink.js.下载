(function($){$.ajax({url:"http://auth.geeksforgeeks.org/gfgLink.php",type:'GET',dataType:"jsonp",data:{'link':window.location.href},success:function(responseData,textStatus,jqXHR){$("#practice").html(responseData.markup);$("#company_tags").html(responseData.tags)},error:function(jqXHR,textStatus,errorThrown){console.log('Response: '+jqXHR)}})})(jQuery);jQuery(document).ready(function($){$.ajax({url:"http://auth.geeksforgeeks.org/plugin.php",method:"POST",dataType:"json",data:{post_id:$("#ratePlugin").attr("pid"),url:window.location.href},xhrFields:{withCredentials:!0},success:function(response){$("#ratePlugin").html(response.rate);$("#markPlugin").html(response.mark)},complete:function(){$(".box").click(function(){rrequest({post_id:$("#ratePlugin").attr("pid"),post_title:$("#ratePlugin").attr("ptitle"),rating:$(this).attr("rating"),url:window.location.href})});$(".mark").click(function(){mrequest({post_id:$("#markPlugin").attr("pid"),post_title:$("#markPlugin").attr("ptitle"),op:$(this).attr("id"),url:window.location.href})})}});function rrequest(payload){$.ajax({url:"http://auth.geeksforgeeks.org/gfgRatings.php",method:"POST",dataType:"html",data:payload,xhrFields:{withCredentials:!0},success:function(response){$("#ratePlugin").html(response)},complete:function(){$(".box").click(function(){rrequest({post_id:$("#ratePlugin").attr("pid"),post_title:$("#ratePlugin").attr("ptitle"),rating:$(this).attr("rating"),url:window.location.href})})}})}function mrequest(payload){$.ajax({url:"http://auth.geeksforgeeks.org/gfgMarking.php",method:"POST",dataType:"html",data:payload,xhrFields:{withCredentials:!0},success:function(response){$("#markPlugin").html(response)},complete:function(){$(".mark").click(function(){mrequest({post_id:$("#markPlugin").attr("pid"),post_title:$("#markPlugin").attr("ptitle"),op:$(this).attr("id"),url:window.location.href})})}})}$('.show-comments').on('click',function(){$.ajax({type:"GET",url:"http://geeksforgeeks.disqus.com/embed.js",dataType:"script",cache:!0});$(this).fadeOut()});});
